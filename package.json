{
  "name": "vsclay",
  "author": "Ramkumar Ramachandra",
  "publisher": "artagnon",
  "repository": {
    "type": "git",
    "url": "https://github.com/artagnon/vsclay"
  },
  "license": "CC0",
  "displayName": "vsclay",
  "description": "Syntax definition for claytext (gh:artagnon/clayoven)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "claytext",
        "extensions": [
          ".clay"
        ],
        "configuration": "./syntax/syntax.json"
      }
    ],
    "grammars": [
      {
        "language": "claytext",
        "scopeName": "text.claytext",
        "path": "./syntax/claytext.tmGrammar.json",
        "embeddedLanguages": {
          "meta.embedded.latex": "latex"
        }
      }
    ]
  },
  "extensionDependencies": [
    "james-yu.latex-workshop",
    "gruntfuggly.triggertaskonsave"
  ],
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
