{
  "name": "vsclay",
  "icon": "assets/clayoven.png",
  "author": {
    "name": "Ramkumar Ramachandra",
    "email": "artagnon@gmail.com",
    "url": "https://artagnon.com"
  },
  "publisher": "artagnon",
  "repository": {
    "type": "git",
    "url": "https://github.com/artagnon/vsclay"
  },
  "license": "CC0-1.0",
  "displayName": "vsclay",
  "description": "Syntax definition for claytext (gh:artagnon/clayoven)",
  "version": "0.0.20",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "claytext",
        "extensions": [
          ".clay"
        ],
        "configuration": "claytext.configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "claytext",
        "path": "./snippets/latex.json"
      }
    ],
    "grammars": [
      {
        "language": "claytext",
        "scopeName": "text.claytext",
        "path": "./syntax/claytext.tmGrammar.json"
      },
      {
        "scopeName": "latex.injection",
        "path": "./syntax/latexInjection.tmGrammar.json",
        "injectTo": [
          "text.claytext"
        ],
        "embeddedLanguages": {
          "meta.embedded.latex": "latex"
        }
      },
      {
        "scopeName": "coq.injection",
        "path": "./syntax/coqInjection.tmGrammar.json",
        "injectTo": [
          "text.claytext"
        ],
        "embeddedLanguages": {
          "meta.embedded.coq": "coq"
        }
      },
      {
        "scopeName": "cpp.injection",
        "path": "./syntax/cppInjection.tmGrammar.json",
        "injectTo": [
          "text.claytext"
        ],
        "embeddedLanguages": {
          "meta.embedded.cpp": "cpp"
        }
      },
      {
        "scopeName": "rs.injection",
        "path": "./syntax/rsInjection.tmGrammar.json",
        "injectTo": [
          "text.claytext"
        ],
        "embeddedLanguages": {
          "meta.embedded.rs": "rust"
        }
      },
      {
        "scopeName": "hs.injection",
        "path": "./syntax/hsInjection.tmGrammar.json",
        "injectTo": [
          "text.claytext"
        ],
        "embeddedLanguages": {
          "meta.embedded.hs": "haskell"
        }
      }
    ]
  },
  "extensionDependencies": [
    "gruntfuggly.triggertaskonsave"
  ],
  "dependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@types/vscode": "1.49.0",
    "eslint": "^7.10.0",
    "typescript": "^4.0.3"
  }
}
