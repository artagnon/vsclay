{
  "brackets": [
    ["$$", "$$"],
    ["{{", "}}"],
    ["[[", "]]"],
    ["<<", ">>"]
  ],
  "autoClosingPairs": [
    { "open": "`", "close": "`" },
    { "open": "$$", "close": "$$" },
    { "open": "{{", "close": "}}" },
    { "open": "[[", "close": "]]" },
    { "open": "<<", "close": ">>" }
  ],
  "autoSurroundingPairs": [
    ["$$", "$$"],
    ["{{", "}}"],
    ["[[", "]]"],
    ["<<", ">>"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^([[|{{)$",
    "decreaseIndentPattern": "^(]]|}})$"
  }
}
