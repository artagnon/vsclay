{
  "scopeName": "text.claytext",
  "patterns": [
    {
      "begin": "\\[\\[",
      "end": "\\]\\]",
      "contentName": "markup.raw.verbatim.claytext"
    },
    {
      "begin": "<<",
      "end": ">>",
      "contentName": "markup.bold.claytext"
    },
    {
      "begin": "\\$\\$",
      "end": "\\$\\$",
      "contentName": "meta.embedded.block.latex"
    },
    {
      "match": "^(#) (.*)$",
      "captures": {
        "1": {
          "name": "keyword.operator.claytext"
        },
        "2": {
          "name": "markup.heading.2.claytext"
        }
      }
    },
    {
      "match": "^\\(.*\\)$",
      "name": "markup.italic.claytext"
    },
    {
      "match": "^(\\[d+\\]): (.*)$",
      "captures": {
        "1": {
          "name": "language.keyword.operator.claytext"
        },
        "2": {
          "name": "markup.italic.claytext"
        }
      }
    }
  ]
}
