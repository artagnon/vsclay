{
  "scopeName": "text.claytext",
  "patterns": [
    {
      "begin": "\\[\\[",
      "end": "\\]\\]",
      "beginCaptures": { "0": { "name": "keyword.operator.claytext" } },
      "endCaptures": { "0": { "name": "keyword.operator.claytext" } },
      "contentName": "markup.quote.claytext"
    },
    {
      "begin": "<<",
      "end": ">>",
      "beginCaptures": { "0": { "name": "keyword.operator.claytext" } },
      "endCaptures": { "0": { "name": "keyword.operator.claytext" } },
      "contentName": "markup.bold.claytext"
    },
    {
      "begin": "\\$\\$",
      "end": "\\$\\$",
      "beginCaptures": { "0": { "name": "keyword.operator.claytext" } },
      "endCaptures": { "0": { "name": "keyword.operator.claytext" } },
      "contentName": "meta.embedded.latex"
    },
    {
      "match": "\\$[^\\$+]\\$",
      "name": "meta.embedded.latex"
    },
    {
      "match": "(\\[)([^\\]]+)(\\])\\([^\\)]+\\)",
      "captures": {
        "1": { "name": "keyword.operator.claytext" },
        "2": { "name": "markup.italic.claytext" },
        "3": { "name": "keyword.operator.claytext" }
      }
    },
    {
      "match": "(`)([^`]+)(`)",
      "captures": {
        "1": { "name": "keyword.operator.claytext" },
        "2": { "name": "markup.quote.claytext" },
        "3": { "name": "keyword.operator.claytext" }
      }
    },
    {
      "match": "^(#) (.+)$",
      "captures": {
        "1": { "name": "keyword.operator.claytext" },
        "2": { "name": "markup.heading.2.claytext" }
      }
    },
    {
      "match": "^\\(.+\\)$",
      "name": "markup.italic.claytext"
    },
    {
      "match": "^\\[\\^\\d+\\]:",
      "name": "keyword.other.claytext"
    }
  ]
}
