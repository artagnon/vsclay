{
  "scopeName": "text.claytext",
  "patterns": [
    {
      "begin": "(\\[\\[|~~)",
      "end": "(\\]\\]|~~)",
      "beginCaptures": { "0": { "name": "keyword.control.claytext" } },
      "endCaptures": { "0": { "name": "keyword.control.claytext" } },
      "contentName": "markup.raw.claytext"
    },
    {
      "begin": "<<",
      "end": ">>",
      "beginCaptures": { "0": { "name": "keyword.control.claytext" } },
      "endCaptures": { "0": { "name": "keyword.control.claytext" } },
      "contentName": "markup.underline.link.claytext"
    },
    {
      "begin": "\\+\\+",
      "end": "\\+\\+",
      "beginCaptures": { "0": { "name": "keyword.control.claytext" } },
      "endCaptures": { "0": { "name": "keyword.control.claytext" } },
      "contentName": "markup.heading.4.claytext"
    },
    {
      "begin": "^\\.\\.\\.$",
      "end": "^\\.\\.\\.$",
      "beginCaptures": { "0": { "name": "keyword.control.claytext" } },
      "endCaptures": { "0": { "name": "keyword.control.claytext" } },
      "contentName": "markup.italic.claytext"
    },
    {
      "begin": "^((\\$\\$)|(\\{\\{))$",
      "end": "^((\\$\\$)|(\\}\\}))$",
      "beginCaptures": { "0": { "name": "keyword.control.claytext" } },
      "endCaptures": { "0": { "name": "keyword.control.claytext" } },
      "contentName": "meta.embedded.latex"
    },
    {
      "begin": "\\$",
      "end": "\\$",
      "beginCaptures": { "0": { "name": "keyword.control.claytext" } },
      "endCaptures": { "0": { "name": "keyword.control.claytext" } },
      "contentName": "meta.embedded.latex"
    },
    {
      "begin": "`",
      "end": "`",
      "beginCaptures": { "0": { "name": "keyword.control.claytext" } },
      "endCaptures": { "0": { "name": "keyword.control.claytext" } },
      "contentName": "markup.quote.claytext"
    },
    {
      "match": "^((\\+)|(\\([ivx]+\\))|(\\d+\\.)|(\\([a-z]\\))|(\\[\\^\\d+\\]:)|(\\-)) ",
      "name": "keyword.control.claytext"
    },
    {
      "match": "^(#) (.+)$",
      "captures": {
        "1": { "name": "keyword.control.claytext" },
        "2": { "name": "markup.heading.2.claytext" }
      }
    },
    {
      "match": "^--$",
      "name": "keyword.control.claytext"
    }
  ]
}
